{
  "name": "react-slice-kit",
  "version": "0.0.1",
  "private": true,
  "dll": [
    "react",
    "react-dom",
    "react-loadable",
    "react-spring",
    "redux",
    "redux-thunk",
    "react-redux",
    "immutable",
    "react-router-dom",
    "history",
    "axios",
    "mix-color",
    "lodash"
  ],
  "scripts": {
    "clear": "rm -rf node_modules yarn.lock && yarn install && yarn dll",
    "deploy": "yarn build && rsync -av build/* root@0.0.0.0:/db/static/resct-slice-kit",
    "dll": "webpack --progress --config scripts/webpack.dll.js",
    "build": "prod=1 webpack --config scripts/webpack.config.js",
    "start": "webpack-dev-server --config scripts/webpack.config.js",
    "lint": "node_modules/.bin/eslint 'src/**/*.js' --cache --cache-location './node_modules/.cache/.eslint_cache/'",
    "test": "jest"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "history": "^4.7.2",
    "immutable": "^4.0.0-rc.10",
    "lodash": "^4.17.11",
    "mix-color": "^1.0.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-loadable": "^5.5.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-spring": "^5.9.2",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.2",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-stage-0": "^7.0.0",
    "@babel/runtime": "^7.1.2",
    "@types/jest": "^23.3.1",
    "@types/react": "^16.4.16",
    "@types/react-dom": "^16.0.9",
    "@types/react-test-renderer": "^16.0.3",
    "autoprefixer": "^9.0.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "23.4.2",
    "babel-loader": "8.0.0-beta.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-styled-components": "^1.7.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^1.0.0",
    "eslint": "^5.5.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-react": "^7.11.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "fast-uglifyjs-plugin": "^0.3.0",
    "file-loader": "^1.1.11",
    "fs-extra": "^7.0.0",
    "html-webpack-plugin": "^4.0.0-beta.1",
    "jest": "^23.6.0",
    "postcss-flexbugs-fixes": "^3.3.1",
    "postcss-loader": "^2.1.5",
    "pre-commit": "^1.2.2",
    "react-dev-utils": "^5.0.2",
    "react-hot-loader": "^4.3.3",
    "react-test-renderer": "^16.5.2",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.23.1",
    "ts-jest": "^23.1.4",
    "ts-loader": "^4.4.2",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9"
  },
  "pre-commit": [
    "lint"
  ],
  "jest": {
    "preset": "react",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/"
    ],
    "cacheDirectory": ".jest/cache"
  }
}
